; ModuleID = 'MicroC'
source_filename = "MicroC"

@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00"

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %a = alloca i32
  %b = alloca i1
  %c = alloca i1
  store i1 true, i1* %b
  store [3 x i32] [i32 1, i32 2, i32 3], i32* %a
  %b1 = load i1, i1* %b
  store [2 x i1] [i1 %b1, i1 false], i1* %c
  ret i32 0
}
