; ModuleID = 'MicroC'
source_filename = "MicroC"

@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00"
@fmt.1 = private unnamed_addr constant [4 x i8] c"%f\0A\00"

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %a = alloca i32
  %b = alloca double
  store i32 1, i32* %a
  store double 2.500000e-01, double* %b
  %a1 = load i32, i32* %a
  %cst1 = sitofp i32 %a1 to double
  %b2 = load double, double* %b
  %tmp = fadd double %cst1, %b2
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @fmt.1, i32 0, i32 0), double %tmp)
  ret i32 0
}
